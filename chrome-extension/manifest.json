{
    "name": "Demurodas Private Extension",
    "version": "1.0",
    "description": "Private Google Chrome extension for use by Demurodas",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "https://analytics.google.com/*"
            ],
            "js": [
                "/js/loadUsers.js"
            ]
        }
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/DD-16.png",
            "32": "images/DD-32.png",
            "48": "images/DD-48.png",
            "128": "images/DD-128.png"
        }
    },
    "icons": {
        "16": "images/DD-16.png",
        "32": "images/DD-32.png",
        "48": "images/DD-48.png",
        "128": "images/DD-128.png"
    }
}